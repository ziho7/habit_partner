{
  "expo": {
    "name": "Habit Partner",
    "slug": "habit-partner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "habit-partner",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.ziho.habitpartner",
      "usesIcloudStorage": true,
      "entitlements": {
        "com.apple.developer.icloud-container-identifiers": [
          "iCloud.com.ziho.habitpartner"
        ],
        "com.apple.developer.ubiquity-container-identifiers": [
          "iCloud.com.ziho.habitpartner"
        ]
      },
      "RCTAsyncStorageExcludeFromBackup": false,
      
      "buildNumber": "6"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "96119a14-af41-4f46-a9d6-ea5a4b06df51"
      }
    },
    "owner": "ziho"
  }
}
